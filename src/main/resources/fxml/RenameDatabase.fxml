<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane maxHeight="200.0" maxWidth="400.0" minHeight="200.0" minWidth="400.0" prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ControllerRenameDatabase">
   <children>
      <Label layoutX="30.0" layoutY="21.0" prefHeight="32.0" prefWidth="370.0" text="Enter the new name!">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Label layoutX="30.0" layoutY="80.0" prefHeight="17.0" prefWidth="51.0" text="Name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="textField" layoutX="82.0" layoutY="78.0" onKeyPressed="#handleTexFieldKeyPress" prefHeight="25.0" prefWidth="305.0" />
      <Button fx:id="buttonOk" layoutX="235.0" layoutY="153.0" mnemonicParsing="false" onAction="#handleButtonOk" prefWidth="60.0" text="Ok" />
      <Button fx:id="buttonCancel" layoutX="326.0" layoutY="153.0" mnemonicParsing="false" onAction="#handleCancelButton" prefWidth="60.0" text="Cancel" />
      <Label fx:id="label" layoutX="30.0" layoutY="123.0" onKeyPressed="#handleTexFieldKeyPress" prefHeight="17.0" prefWidth="356.0" text="Error" textFill="RED" />
   </children>
</Pane>
